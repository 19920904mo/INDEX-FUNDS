<h1>Knowledgeposts#index</h1>
<p>Find me in app/views/knowledgeposts/index.html.erb</p>
<table>
  <tr>
    <th>Id</th><th >title</th><th>read</th>
      <th>genre</th><th colspan="2"></th>
  </tr>
  <% @data.each do |obj| %>
  <tr>
    <td><%= obj.id %></td>
    <td><%= obj.title %></td>
    <td><%= obj.read %></td>
    <td><%= obj.knowledgegenre.name %></td>
    <td><a href="/knowledgeposts/<%= obj.id %>">Edit</a></td>
    <td><a href="/knowledgeposts/delete/<%= obj.id %>">Delete</a></td>
  </tr>
  <% end %>
</table>
<p><a href="/knowledgeposts/add">新規作成＞＞</a></p>