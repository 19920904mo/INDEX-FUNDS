<h1>Knowledgegenres#add</h1>
<p>Find me in app/views/knowledgegenres/add.html.erb</p>
<% if @knowledgegenre.errors.any? %>
<ul>
    <% @knowledgegenre.errors.full_messages.each do |err| %>
      <li><%= err %></li>
    <% end %>
</ul>
<% end %>
<table>
    <%= form_for(@knowledgegenre, url:{controller:'knowledgegenres', action:'add' }) do |form| %>
    <tr><th>name</th>
        <td><%= form.text_field :name %></td></tr>
    <tr><th>memo</th>    
        <td><%= form.text_field :memo %></td></tr>
    <tr><th></th>
        <td><%= form.submit "送信" %></td></tr>
    <% end %>
</table>
