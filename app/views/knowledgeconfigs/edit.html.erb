<h1>Knowledgeconfigs#edit</h1>
<p>Find me in app/views/knowledgeconfigs/edit.html.erb</p>
<% if @knowledgeconfig.errors.any? %>
<ul>
  <%  @knowledgeconfig.errors.full_messages.each do |err| %>
    <li><%= err %></li>
  <% end %>
</ul>
<% end %>
<table>
  <%= form_for(@knowledgeconfig, url:{controller:'knowledgeconfigs', action:'edit'}) do |form| %>
  <tr><th>Title</th>
      <td><%= form.text_field :title %></td></tr>
  <tr><th>Subtitle</th>
      <td><%= form.text_field :subtitle %></td></tr>
  <tr><th>Style name</th>
      <td><%= form.text_field :stylename %></td></tr>
  <tr><th></th>
      <td><%= form.submit "送信" %></td></tr>
  <% end %>
</table>